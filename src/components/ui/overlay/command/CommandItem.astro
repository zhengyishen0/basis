---
import CommandContent from './CommandContent.astro';

export interface Props {
    class?: string;
    [key: string]: any;
}

const { class: className, ...alpineProps } = Astro.props;
---

<CommandContent class={className} {...alpineProps}>
    <slot name="icon" slot="icon" />
    <slot />
    <slot name="shortcut" slot="shortcut" />
</CommandContent>
